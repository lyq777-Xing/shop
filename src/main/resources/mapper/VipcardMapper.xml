<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.supermarket.mapper.VipcardMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.supermarket.pojo.Vipcard">
        <id column="vipcardid" property="vipcardid" />
        <result column="holderid" property="holderid" />
        <result column="creationdate" property="creationdate" />
        <result column="integral" property="integral" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        vipcardid, holderid, creationdate, integral
    </sql>

</mapper>
