<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.zjitc.mapper.RoleMapper">
    <select id="deleterightsid" parameterType="net.zjitc.entity.Role">

        UPDATE sp_role SET ps_ids=REPLACE(ps_ids,#{rightid},'') WHERE role_id=#{roleid}

    </select>
</mapper>